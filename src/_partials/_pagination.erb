<% if paginator.total_pages > 1 %>
  <div class="pagination">
    <% if paginator.previous_page %>
      <a href="<%= paginator.previous_page_path %>">❮ Newest</a>
    <% end %>
    <% if paginator.next_page %>
      <a href="<%= paginator.next_page_path %>">Previous❯</a>
    <% end %>
  </div>
<% end %>

